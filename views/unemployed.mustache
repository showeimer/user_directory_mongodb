<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>User Directory</title>
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body>
    <h1>Unemployed Users</h1>
    <div class="container">
      {{#userDirectory}}
      <div class="users">
        <a href="/users/{{username}}"><img src="{{avatar}}"></a>
        <h2 id="name">{{name}}</h2>
        <h5 id="job">{{job}} {{company}}</h5>
      </div>
      {{/userDirectory}}
    </div>
  <script src="main.js"></script>
  </body>
</html>
